<h1>Users Blog</h1>
<div class="user-container">
    <ul>
      <%@users.each do |user|%>
        <li>
          <a href="/users/<%= user.id%>">
            <%= render partial: 'partials/users', locals: { user: user } %>
          </a>
        </li>
      <%end%>
    </ul>
    <button type="button" class="button_center"><%= link_to "Post", new_user_post_path( user_id: @user.id), { class: "user-link" } %></button>
</div>