<h1>List of Users</h1>
<div class="user-container">
    <%if @users.empty?%>
        <h1>No users!</h1>
    <%end%>

    <ul>
      <%@users.each do |user|%>
        <li>
          <a href="/users/<%= user.id%>">
            <%= render partial: 'partials/users', locals: { user: user } %>
          </a>
        </li>
      <%end%>
    </ul>
</div>